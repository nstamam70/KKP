	<queryString language="SQL">
		<![CDATA[SELECT 
    pembayaran.Pembayaranid,
    pembayaran.Tanggal,
    pembayaran.NamaAdmin,
    pembayaran.TotalBeli,
    pembayaran.TotalHarga,
    pembayaran.Bayar,
    pembayaran.Kembalian,
    pasien.Pasienid,
    pasien.Nik,
    pasien.Nama AS NamaPasien,
    pasien.TempatLahir,
    pasien.TanggalLahir,
    pasien.Jenkel,
    pasien.Goldar,
    pasien.NoTelp,
    pasien.Alamat,
    obat.Obatid,
    obat.Nama AS NamaObat,
    obat.Kode,
    obat.Satuan,
    obat.Kadaluarsa,
    obat.Stok,
    obat.Harga
FROM 
    pembayaran
INNER JOIN 
    pasien ON pembayaran.Pasienid = pasien.Pasienid
INNER JOIN 
    obat ON pembayaran.Obatid = obat.Obatid;]]>
	</queryString>
