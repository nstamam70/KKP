	<queryString language="SQL">
		<![CDATA[SELECT 
    pemeriksaan.Pemeriksaanid,
    pemeriksaan.Tb,
    pemeriksaan.Bb,
    pemeriksaan.Diagnosa,
    pemeriksaan.keluhan,
    pemeriksaan.TekananDarah,
    pemeriksaan.Tanggal,
    pasien.Pasienid,
    pasien.Nik,
    pasien.Nama AS NamaPasien,
    pasien.TempatLahir,
    pasien.TanggalLahir,
    pasien.Jenkel,
    pasien.Goldar,
    pasien.NoTelp,
    pasien.Alamat,
    dokter.Dokterid,
    dokter.Nama AS NamaDokter,
    dokter.Spesialis,
    dokter.NoTelp AS NoTelpDokter,
    dokter.Alamat AS AlamatDokter,
    obat.Obatid,
    obat.Nama AS NamaObat,
    obat.Kode,
    obat.Satuan,
    obat.Kadaluarsa,
    obat.Stok,
    obat.Harga
FROM 
    pemeriksaan
INNER JOIN 
    pasien ON pemeriksaan.Pasienid = pasien.Pasienid
INNER JOIN 
    dokter ON pemeriksaan.IdDokter = dokter.Dokterid
INNER JOIN 
    obat ON pemeriksaan.Obatid = obat.Obatid;]]>
	</queryString>
